set inputbar-fg                 "#F8F8FA"

set notification-bg             "#1C1E26"
set notification-fg             "#F8F8FA"

set notification-error-bg       "#1C1E26"
set notification-error-fg       "#E57B94"

set notification-warning-bg     "#1C1E26"
set notification-warning-fg     "#E57B94"

set highlight-color             "#F9C199"
